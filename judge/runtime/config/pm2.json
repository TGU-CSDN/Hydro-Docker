{
  "apps": [
    {
      "name": "hydro-sandbox",
      "script": "bash",
      "args": [
        "-c",
        "ulimit -s unlimited && /usr/local/bin/hydro-sandbox -mount-conf /root/.hydro/mount.yaml -http-addr=localhost:5050"
      ],
      "autorestart": true
    },
    {
      "name": "hydro-judge",
      "script": "hydrojudge",
      "autorestart": true
    }
  ]
}